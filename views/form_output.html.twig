<!DOCTYPE html>
  <html>
      <head>
          <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css">
          <link href="css.css" rel="stylesheet" type="text/css">
      </head>
      <body>
    <div class="container">
      <h1 class="jumbotron">Contacts</h1>
        {% for contact in contact_list %}
        <h1>{{contact.name}}</h1>
        <ul>
            <l1>{{contact.address}}</l1>
            <l1>{{contact.phone_number}}</l1>
        </ul>
        {% endfor %}
      <button type="button"><a href="/">Add another contact</a></button>
      <button type="button"><a href="/clear">Clear contacts</a></button>
    </div>
      </body>
  </html>
