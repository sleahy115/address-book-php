<!DOCTYPE html>
  <html>
      <head>
          <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css">
          <link href="css.css" rel="stylesheet" type="text/css">
      </head>
      <body>
        <div class="container">
          <div class="jumbotron">Contacts</div>
            {% for contact in contact_list %}
            <div class="panel panel-default">
                <div class="panel-heading" id="heading">
                    <h1>{{contact.getName()}}</h1>
                </div>
                <div class="panel-body">
                    <ul>
                        <li>Address:{{contact.getAddress()}}</li>
                        <li>Phone:{{contact.getPhoneNumber()}}</li>
                        <li>Email:{{contact.getEmail()}}</li>
                    </ul>
                </div>
            </div>
            {% endfor %}
          <button type="button"><a href="/">Add another contact</a></button>
          <button type="button"><a href="/clear">Clear all contacts</a></button>
        </div>
      </body>
  </html>
